    "gatsby-transformer-sharp": "^2.3.6",
    "intersection-observer": "^0.7.0",
    "jsdom": "^15.2.1",
    "lottie-web": "^5.6.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
